Title : Program on dealing with Arrays and Sorting of Array
 }
 cout<<"\nSorting the Array in ascending order\n";
 for(int i=0;i<(n-1);i++)
 {
 for(int j=0;j<(n-i-1);j++)
 {
 if(a[j]>a[j+1])
 {
 temp=a[j];
 a[j]=a[j+1];
 a[j+1]=temp;
 }
 }
 }
 cout<<"Array sorted successfully\n";
 cout<<"The array elements after sorting are :\n";
 for (int i=0;i<n;i++)
 {
 cout<<a[i]<<endl;
 }
 return 0;
}
