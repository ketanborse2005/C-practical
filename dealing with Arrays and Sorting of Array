#include<iostream>
using namespace std;
int main()
{
 int a[10],n,j,temp;
 cout<<"Enter the number of elements :\n";
 cin>>n;
 cout<<"Enter thr array elements \n";
 for(int i=0;i<n;i++)
 {
 cin>>a[i];
 }
 cout<<"The array elements are :\n";
 for (int i=0;i<n;i++)
 {
 cout<<a[i]<<endl;
